
#include <iostream>

using namespace std;
int main()
{
	setlocale(LC_ALL, "rus");
	int cells = 0;
	int cells2 = 0;
	

	
	for (int i = 0; i < 32; i++)
	{
		cout << "***";
		cout << "___"; 
		cells++;
		if (cells % 8 == 0)
		{
			cout << endl;
		}
	}
	for ( int i = 0; i < 32; i++)
	{
		cout << "___";
		cout << "***";
		cells2++;
		if (cells2 % 8 == 0)
		{
			cout << endl;
		}
	}

	
}


